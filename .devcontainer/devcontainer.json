
{
  "name": "Moonshot Demos",
  "image": "mcr.microsoft.com/devcontainers/python:3.10",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {},
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.10"
    }
  },
  "postCreateCommand": "pip install --upgrade pip && pip install streamlit openai pandas scikit-learn plotly",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "forwardPorts": [8501],
  "remoteUser": "vscode"
}

